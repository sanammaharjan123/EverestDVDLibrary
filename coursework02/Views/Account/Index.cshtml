@model IEnumerable<coursework02.Models.UpdateViewModel>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<table class="table table-bordered">
    <tr>
        <th>
          UserName
        </th>
        <th>
          Email
        </th>
        <th>
         Phone Number
        </th>
        <th>
          Role
        </th>
       <th></th>
        
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
          @item.UserName
        </td>
        <td>
         @item.Email 
        </td>
        <td>
          @item.PhoneNumber 
        </td>
        <td>
          @item.UserRoles
        </td>
       <th>@Html.ActionLink("EditUser","EditUser",new {id=item.UserId })</th>
       
    </tr>
}

</table>
